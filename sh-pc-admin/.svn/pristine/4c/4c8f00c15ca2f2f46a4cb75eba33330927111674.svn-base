        <style type="text/css">
            *{
                margin:0;
                padding:0;
                list-style:none;
                box-sizing: border-box;
            }
            .time_line{
                display:flex;
                margin-top:10px;
            }
            .time_line li{
                flex:1;
                position:relative;
            }
            .time_line li:after{
                content:"";
                position:absolute;
                z-index:99;
                top:30px;
                left:-50%;
                width:100%;
                height:5px;
                background:#ddd;

            }
            .time_line li:first-child:after{
                display:none;
            }
            .time{
                text-align:center;
                margin-top:80px;
                margin-bottom:10px;
                color:#999;
            }
            .state{
                position:absolute;
                z-index:100;
                width:60px;
                height:60px;
                font-size:14px;
                line-height:60px;
                text-align:center;
                background:#ccc;
                border-radius:50%;
                color:#fff;
                left:50%;
                margin-left:-30px;
            }
            .active .state{
                background:#009688;
            }
            .time_line .active:after{
                background:#009688;
            }
        </style>
	</head>

	<body>
    <ul class="time_line">
        <li>
            <div class="state">
